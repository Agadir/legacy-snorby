<%= box_header "New User Setup" -%>

<% form_for @user, :url => account_path, :html => { :multipart => true, :id => 'user_form' } do |f| %>
  <%= render :partial => "form", :object => f %><br />
  <br />
	<%= f.submit "Create User" %>
	<%= f.submit "Cancel", :onclick => 'history.go(-1);return false;' %>
<% end %>