<div id="header">
	<%- if current_user -%>
	<div id="menubar">
		<ul class="menu">
			<li><%= link_to "Dashboard", dashboard_path %></li>
			<li><%= link_to "LiveLook", livelook_path %></li>
			<li><%= link_to "Events", events_path %></li>
			<li><%= link_to "Reports", reports_path %></li>
			<li><%= link_to "Search", new_search_path %></li>
			<li><%= link_to "Settings", settings_path %></li>
			<% if !current_user %>
			  <li><%= link_to "Log In", login_path %></li>
			<% else %>
			  <li><%= link_to "Logout", logout_path, :method => :delete, :confirm => "Are you sure you want to logout?" %></li>
			<% end %>
		</ul>
	</div>
	
	<div id="logo"><%= link_to image_tag('snorby_logo.png'), dashboard_path %></div>
	
	<%- else -%>

	<div id="logo"><%= link_to image_tag('snorby_logo.png'), login_path %></div>
	
	<%- end -%>


</div>